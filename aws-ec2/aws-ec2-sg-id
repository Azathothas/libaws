#!/usr/bin/env python3
import argh
import cli_aws

def main(name):
    xs = [x for x in cli_aws.ec2_sgs() if x.group_name == name]
    assert len(xs) == 1, 'didnt find exactly one match: %s' % xs
    return xs[0].group_id

if __name__ == '__main__':
    with cli_aws.setup():
        argh.dispatch_command(main)
